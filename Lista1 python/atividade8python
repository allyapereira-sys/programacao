contador = 1
soma_notas =0
while contador <=4:
    nota = float(input(f"insira a nota do{contador}aluno"))
    if nota< 0 or nota > 10:
        print("nota invalida. a nota devera estar entre 9 e 10")
        continue
    soma_notas += nota
    contador += 1
media=soma_notas/4
print("media dos alunos Ã©:",media)

